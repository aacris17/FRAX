)

DM_percent_DXA <- (nrow(Fracture_Dates_fx %>%
                          filter(DM == 'TRUE' & 
                                   DXA == 'TRUE')
                        )/nrow(Fracture_Dates_fx %>% 
                            filter(DM == 'TRUE')))*100

