#Type of fracture
Hip_fracture <- Fracture_Dates %>% filter(grepl("[Ff]emur", DESC))
Hip <- Hip_fracture %>% count() %>% as.double()
perc_hip <- (Hip/nrow(Fracture_Dates))*100 %>% as.double()
Major_fracture <- Fracture_Dates %>% filter(grepl("[Ss]pine", DESC)| grepl("[Ww]rist", DESC)| grepl("[Hh]umerus", DESC))
Major <- Major_fracture %>% count() %>% as.double()
perc_major <- (Major/nrow(Fracture_Dates))*100 %>% as.double()

```{r missing_data}
#Change true, false or non to select the population who is missing data
test <- Fracture_Dates_fx %>% filter(DM == "FALSE")

tot_na <- (test %>% filter(is.na(Risk)) %>% nrow())/nrow(test)
CODE_na <- (test %>% filter(is.na(CODE)) %>% nrow())/nrow(test)
HT_na <- (test %>% filter(is.na(HT.in)) %>% nrow())/nrow(test)
WT_na <- (test %>% filter(is.na(WT.in)) %>% nrow())/nrow(test)
AGE_na <- (test %>% filter(is.na(AGE)) %>% nrow())/nrow(test)
GENDER_na <- (test %>% filter(is.na(GENDER)) %>% nrow())/nrow(test)
Og_RACE_na <- (test %>% filter(is.na(Og_RACE)) %>% nrow())/nrow(test)

missing_data <- tibble(
  total = as.double(tot_na),
  Diagnosis = as.double(CODE_na),
  Height = as.double(HT_na),
  Weight = as.double(WT_na),
  Age = as.double(AGE_na),
  Sex = as.double(GENDER_na),
  Race = as.double(Og_RACE_na)
)
write.csv(missing_data, "C:\\Users\\acriscit\\Downloads\\missing_data.csv")
rm(missing_data)
rm(test)

```

```{r stats}
#Add DM 'T/F' column to Fracture_Dates_fx
DM_Dates_fx <- DM_dates_fx
DM_Dates_fx <- DM_Dates_fx %>%
  mutate(DM = 'DM')
Fracture_Dates_fx <- DM_Dates_fx %>%
  select(ID, DM) %>%
  right_join(Fracture_Dates_fx, by = 'ID')
Fracture_Dates_fx$DM <-  ifelse(is.na(Fracture_Dates_fx$DM),
                                'FALSE', 'TRUE')

Risk_v <- tibble(
  mod  = character(),
  data.name = character(),
  p.value = numeric(),
  conf.int1 = numeric(),
  conf.int2 = numeric(),
  estimate1 = numeric(),
  estimate2 = numeric(),
  n = numeric()
)

#Gender
Men <- Fracture_Dates_fx_no_DM %>% filter(GENDER == 'M') %>% select(GENDER) %>% count() %>% as.double()
Percent_Men <- (Men / nrow(Fracture_Dates_fx_no_DM))*100

Men_DM <- DM_Dates_fx %>% filter(GENDER == 'M') %>% select(GENDER) %>% count() %>% as.double()
Percent_Men_DM <- (Men_DM / nrow(DM_Dates_fx))*100

#% on Osteoporosis medications
#Need to get rid of incorrect medications
#Fracture_Dates_fx_on_meds <- Fracture_Dates_fx %>% filter(osteo_med_name != "Estrogen receptor positive status [ER+]" & osteo_med_name != "Estrogen receptor negative status [ER-]")

on_meds <- nrow(Fracture_Dates_fx_on_meds)/nrow(Fracture_Dates_fx)

Fracture_Dates_fx <- Fracture_Dates_fx %>% mutate(oste_med = ifelse(is.na(osteo_med_name), "N", "Y"))

#Osteo meds vs FRAX score
G_v_Risk <- Fracture_Dates_fx %>% 
  {t.test(.$Risk ~ .$oste_med, alternative = c("two.sided"),
       mu = 0, paired = FALSE, var.equal = FALSE,
       conf.level = 0.95)}

Risk_v <- Risk_v %>% add_row(
  mod  = 'NA',
  data.name = G_v_Risk$data.name,
  p.value = G_v_Risk$p.value,
  conf.int1 = G_v_Risk$conf.int[1],
  conf.int2 = G_v_Risk$conf.int[2],
  estimate1 = G_v_Risk$estimate[1],
  estimate2 = G_v_Risk$estimate[2],
  n = nrow(Fracture_Dates_fx)
)

#Gender vs FRAX score
G_v_Risk <- Fracture_Dates_fx %>% 
  {t.test(.$Risk ~ .$GENDER, alternative = c("two.sided"),
       mu = 0, paired = FALSE, var.equal = FALSE,
       conf.level = 0.95)}

Risk_v <- Risk_v %>% add_row(
  mod  = 'NA',
  data.name = G_v_Risk$data.name,
  p.value = G_v_Risk$p.value,
  conf.int1 = G_v_Risk$conf.int[1],
  conf.int2 = G_v_Risk$conf.int[2],
  estimate1 = G_v_Risk$estimate[1],
  estimate2 = G_v_Risk$estimate[2],
  n = nrow(Fracture_Dates_fx)
)

#Race
#T.test Risk vs Race
G_v_Risk <- Fracture_Dates_fx %>% 
  filter(Og_RACE == 'B'| Og_RACE == 'W') %>%
  {t.test(.$Risk ~ .$Og_RACE, alternative = c("two.sided"),
       mu = 0, paired = FALSE, var.equal = FALSE,
       conf.level = 0.95)}

Risk_v <- Risk_v %>% add_row(
  mod  = 'NA',
  data.name = G_v_Risk$data.name,
  p.value = G_v_Risk$p.value,
  conf.int1 = G_v_Risk$conf.int[1],
  conf.int2 = G_v_Risk$conf.int[2],
  estimate1 = G_v_Risk$estimate[1],
  estimate2 = G_v_Risk$estimate[2],
  n = nrow(Fracture_Dates_fx)
)

#Age of fracture vs Race
G_v_Risk <- Fracture_Dates_fx %>% 
  filter(Og_RACE == 'B'| Og_RACE == 'W') %>%
  {t.test(.$AGE ~ .$Og_RACE, alternative = c("two.sided"),
       mu = 0, paired = FALSE, var.equal = FALSE,
       conf.level = 0.95)}
#Note that group 1 is B and group 2 is W. I think they are sorted alphabetically
Risk_v <- Risk_v %>% add_row(
  mod  = 'NA',
  data.name = G_v_Risk$data.name,
  p.value = G_v_Risk$p.value,
  conf.int1 = G_v_Risk$conf.int[1],
  conf.int2 = G_v_Risk$conf.int[2],
  estimate1 = G_v_Risk$estimate[1],
  estimate2 = G_v_Risk$estimate[2],
  n = nrow(Fracture_Dates_fx %>% 
  filter(Og_RACE == 'B'| Og_RACE == 'W'))
)

#BMI v Race
G_v_Risk <- Fracture_Dates_fx %>% 
  filter(Og_RACE == 'B'| Og_RACE == 'W') %>%
  filter(BMI < 100) %>%
  {t.test(.$BMI ~ .$Og_RACE, alternative = c("two.sided"),
       mu = 0, paired = FALSE, var.equal = FALSE,
       conf.level = 0.95)}
#Note that group 1 is B and group 2 is W. I think they are sorted alphabetically
Risk_v <- Risk_v %>% add_row(
  mod  = 'NA',
